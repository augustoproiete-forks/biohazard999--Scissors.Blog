---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>blog.delegate.at</title>
  <link href="http://blog.delegate.at/"/>
  <link type="application/atom+xml" rel="self" href="http://blog.delegate.at/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://blog.delegate.at/</id>
  <author>
    <name>Manuel Grundner</name>
    <email>m.grundner@delegate.at</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://blog.delegate.at{{ post.url }}</id>
    <link type="text/html" rel="alternate" href="http://blog.delegate.at{{ post.url }}"/>
    <title>{{ post.title | xml_escape }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Manuel Grundner</name>
      <uri>http://blog.delegate.at/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>